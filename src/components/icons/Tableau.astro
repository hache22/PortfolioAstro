---
const { class: className } = Astro.props;
---
<svg
    xmlns="http://www.w3.org/2000/svg"
    class={className}
    viewBox="0 0 24 24"
    fill="currentColor"
>
    <!-- Marco del dashboard -->
    <rect x="2" y="2" width="20" height="20" rx="2" fill="none" stroke="currentColor" stroke-width="1.5"/>
    
    <!-- Barra superior -->
    <rect x="2" y="2" width="20" height="3" fill="currentColor" opacity="0.7"/>
    
    <!-- Gráficos y elementos del dashboard -->
    <rect x="3.5" y="6.5" width="7" height="5" rx="1" fill="currentColor" opacity="0.6"/>
    <rect x="13.5" y="6.5" width="7" height="5" rx="1" fill="currentColor" opacity="0.8"/>
    <rect x="3.5" y="13.5" width="7" height="5" rx="1" fill="currentColor" opacity="0.8"/>
    <rect x="13.5" y="13.5" width="7" height="5" rx="1" fill="currentColor" opacity="0.6"/>
    
    <!-- Líneas de gráfico -->
    <polyline points="5,8.5 6,7.5 7,9 8.5,8" stroke="currentColor" stroke-width="0.5" fill="none"/>
    <polyline points="15,8.5 16,7 17,8 18.5,7.5" stroke="currentColor" stroke-width="0.5" fill="none"/>
    
    <!-- Barras de gráfico -->
    <rect x="5" y="15.5" width="1" height="2" fill="currentColor"/>
    <rect x="6.5" y="14.5" width="1" height="3" fill="currentColor"/>
    <rect x="8" y="16" width="1" height="1.5" fill="currentColor"/>
    
    <!-- Gráfico circular -->
    <path d="M16.5,16 L16.5,18 A1.5,1.5 0 0,0 18,16.5 Z" fill="currentColor"/>
    <circle cx="16.5" cy="16.5" r="1.5" fill="none" stroke="currentColor" stroke-width="0.5"/>
</svg>